    "@graphql-tools/merge": "8.3.6",
    "@graphql-tools/schema": "9.0.4",
    "deepmerge": "^4.2.2",
    "framer-motion": "^7.6.10",
    "graphql": "16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "^13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/cli": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "29.0.3",
    "@types/mocha": "9.1.1",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-jest": "29.0.3",
    "babel-loader": "^8.3.0",
    "cspell": "6.10.1",
    "dotenv": "16.0.2",
    "eslint": "8.22.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-storybook": "^0.6.8",
    "fixpack": "4.0.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "yaml-loader": "0.8.0"
  },
  "private": true,
  "scripts": {
    "build": "yarn codegen && next build",
    "build-storybook": "build-storybook",
    "codegen": "graphql-codegen --require dotenv/config -r ts-node/register -c codegen-json.yaml && graphql-codegen -r ts-node/register -c codegen.yml",
    "confirm": "run-p confirm:*",
    "confirm:format": "prettier '*/**/*.{js,ts,tsx}'",
    "confirm:lint": "yarn eslint '*/**/*.{js,ts,tsx}'",
    "confirm:spell": "cspell '*/**/*.{js,ts,tsx}'",
    "dev": "yarn codegen && next",
    "fix": "run-p fix:*",
    "fix:format": "prettier '*/**/*.{js,ts,tsx}' --write",
    "fix:lint": "yarn eslint  --fix '*/**/*.{js,ts,tsx}'",
    "fix:pack": "fixpack",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "yarn codegen && jest"
  }
}
